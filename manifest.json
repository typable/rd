{
  "name": "reader",
  "description": "A lightweight reader mode extension",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [{
    "js": ["./src/js/content-script.js"],
    "matches": ["<all_urls>"]
  }],
  "web_accessible_resources": [{
      "resources": ["*.js", "*.json"],
      "matches": ["<all_urls>"]
  }]
}
